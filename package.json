  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:debug": "nodemon --inspect=0.0.0.0:9229 --exec node -r ts-node/register src/index.ts",
